{
  "ID": "Info",
  "layout": {
      "widget": "page",
      "children": [{
          "widget": "container",
          "layout": "vertical",
          "text": "User Information",
          "children": [{
                "widget": "display",
                      "display": "{\"user\": user, \"_dj_icons\": \"account_circle\"}"
                      }, {
                        "widget": "display",
                        "display": "{\"email\": email}"
                      }, {
                        "widget": "display",
                      "display": "{\"roles\": $djRoles(), \"_dj_icons\":\"person_search\"}"
                      }, {
                        "widget": "text",
                        "text": "System Roles",
                        "href": "/table/config/dj-role",
                        "icon": "people_outline"
                      }, {
                        "widget": "container",
                        "children": [{
                          "widget": "text",
                          "text": "Tenant Users",
                          "href": "/table/config/tenantusers",
                          "icon": "people"
                        }],
                        "if": "$contains(href, \".my.dashjoin.\")"
                      }],
                      "title": "User Information"
                          }, {
                            "widget": "display",
                            "display": "($x:=$djVersion();\n$merge( [$x,\n{\"Third Party Licenses\": \"https://download.dashjoin.com/dashjoin/release/\" & $substringBefore(x.version, '-') & \"/license-report.html\"},\n{\"_dj_icons\":{\n\"version\":\"content_copy\",\n\"runtime\":\"build\",\n\"buildTime\":\"access_time\",\n\"vendor\":\"store\",\n\"name\":\"check_circle_outline\",\n\"title\":\"title\",\n\"Third Party Licenses\":\"copyright\"\n}\n}])\n)",
                            "title": "Software"
              }, {
                "widget": "display",
                "display": "$djGetDatabases()",
                "title": "Database Drivers"
              }, {
                "widget": "display",
                "display": "$djGetDrivers()",
                "title": "JDBC Drivers"
              }, {
                "widget": "display",
                "display": "$djGetFunctions()",
                "title": "Functions"
              }],
              "pageLayout": "2 column"
  }
}